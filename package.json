{
  "name": "cms-nest-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "https://github.com/anonnoisy",
  "private": true,
  "license": "MIT Licensed",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "dotenv -e .env.local -- node dist/main",
    "start:dev": "dotenv -e .env.development.local -- node dist/main",
    "start:prod": "dotenv -e .env.production.local -- node dist/main",
    "start:debug": "nest start --debug -b swc -w",
    "watch": "dotenv -e .env.local -- nest start -b swc -w",
    "watch:dev": "dotenv -e .env.development.local -- nest start -b swc -w",
    "watch:prod": "dotenv -e .env.production.local -- nest start -b swc -w",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generate": "npx prisma generate",
    "migrate": "dotenv -e .env.local -- npx prisma migrate dev",
    "migrate:dev": "dotenv -e .env.development.local -- npx prisma migrate dev",
    "migrate:production": "dotenv -e .env.production.local -- npx prisma migrate dev",
    "migrate:create": "dotenv -e .env.local -- npx prisma migrate dev --create-only",
    "migrate:deploy": "dotenv -e .env.local -- npx prisma migrate deploy",
    "migrate:resolve-rb": "dotenv -e .env.local -- npx prisma migrate resolve --rolled-back",
    "migrate:resolve-apl": "dotenv -e .env.local -- npx prisma migrate resolve --applied",
    "migrate:reset": "dotenv -e .env.local -- npx prisma migrate reset",
    "dbpush": "dotenv -e .env.local -- npx prisma generate && npx prisma db push",
    "dbpush:dev": "dotenv -e .env.development.local -- npx prisma generate && npx prisma db push",
    "dbpush:production": "dotenv -e .env.production.local -- npx prisma generate && npx prisma db push",
    "seed": "dotenv -e .env.local -- npx prisma db seed",
    "seed:dev": "dotenv -e .env.development.local -- npx prisma db seed",
    "deploy": "npx prisma generate && nest build && pm2 startOrRestart ecosystem.config.js",
    "service:up": "docker compose -f docker/docker-compose.service.yml --env-file .env.local up -d --build",
    "service:down": "docker compose -f docker/docker-compose.service.yml --env-file .env.local down",
    "deploy:up": "docker compose -f docker/docker-compose.prod.yml --env-file .env up -d --build",
    "deploy:down": "docker compose -f docker/docker-compose.prod.yml --env-file .env down"
  },
  "prisma": {
    "schema": "./src/infrastructures/database/prisma/schema",
    "seed": "ts-node ./src/infrastructures/database/prisma/seeders/index.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/s3-request-presigner": "^3.758.0",
    "@fastify/compress": "^8.0.1",
    "@fastify/csrf-protection": "^7.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/multipart": "^9.0.3",
    "@fastify/static": "^8.1.1",
    "@keyv/redis": "^4.3.1",
    "@nestjs-cls/transactional": "^2.5.0",
    "@nestjs-cls/transactional-adapter-prisma": "^1.2.16",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^11.0.11",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.11",
    "@nestjs/devtools-integration": "^0.2.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.0.11",
    "@nestjs/swagger": "^11.0.6",
    "@opentelemetry/exporter-trace-otlp-http": "^0.57.2",
    "@opentelemetry/instrumentation": "^0.57.2",
    "@opentelemetry/resources": "^1.30.1",
    "@opentelemetry/sdk-trace-base": "^1.30.1",
    "@opentelemetry/sdk-trace-node": "^1.30.1",
    "@opentelemetry/semantic-conventions": "^1.30.0",
    "@prisma/client": "6.4.1",
    "@prisma/instrumentation": "^6.4.1",
    "@scalar/nestjs-api-reference": "^0.3.186",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.41.7",
    "cache-manager": "^6.4.1",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "fastify": "^5.2.1",
    "firebase-admin": "^13.1.0",
    "ioredis": "^5.5.0",
    "joi": "^17.13.3",
    "keyv": "^5.3.1",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "mime-types": "^2.1.35",
    "mqtt": "^5.10.4",
    "nestjs-cls": "^5.4.0",
    "nestjs-prisma": "^0.25.0",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
